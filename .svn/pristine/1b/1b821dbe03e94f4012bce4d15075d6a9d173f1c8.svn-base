package cn.sxt.bus.utils;

import java.util.List;

import cn.sxt.sys.constast.SYS_Constast;

/**
 * 适应easyui的表格需要的json对象
 * @author LJH
 *
 */
public class DataGridView {
	
	private  Long code=Long.valueOf(SYS_Constast.TYPE_PUBLIC_ZERO);
	
	private  String msg="";
	
	private Long count;
	
	private List<?> data;

	public Long getCode() {
		return code;
	}

	public String getMsg() {
		return msg;
	}

	public Long getCount() {
		return count;
	}

	public void setCount(Long count) {
		this.count = count;
	}

	public List<?> getData() {
		return data;
	}

	public void setData(List<?> data) {
		this.data = data;
	}

	public DataGridView() {
		super();
		// TODO Auto-generated constructor stub
	}

	public DataGridView(Long count, List<?> data) {
		super();
		this.count = count;
		this.data = data;
	}

	

}
